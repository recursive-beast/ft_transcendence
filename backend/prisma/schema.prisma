generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              Int      @id
  login           String
  first_name      String
  last_name       String
  usual_full_name String
  image           String
  created_at      DateTime @default(now())
  updated_at      DateTime @updatedAt
}
