generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id             Int      @id @default(autoincrement())
  forty_two_id   String?
  google_id      String?
  display_name   String   @unique
  full_name      String
  image          String?
  otp_secret     String?
  otp_is_enabled Boolean  @default(false)
  created_at     DateTime @default(now())
  updated_at     DateTime @updatedAt
}
